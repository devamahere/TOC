    "#dfa for consecutive three b's\n",
    "s=input()\n",
    "flag=0\n",
    "for i in range(2,len(s)):\n",
    "    if s[i]=='b' and s[i-1]=='b' and s[i-2]=='b':\n",
    "        flag=1\n",
    "        break\n",
    "if flag==1 :\n",
    "    print(\"valid dfa\")\n",
    "else :\n",
    "    print(\"invalid dfa\")"
